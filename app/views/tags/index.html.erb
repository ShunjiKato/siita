<% provide(:title, "タグ一覧") %>
<div class="row">
  <% @tags.each_with_index do |tag, index| %>
    <% if index % 5 == 0 %>
      <div class="row">
    <% end %>
      <div class="col-md-3">
      <tr>
        <td><%= tag.name %></td>
      </tr>
    <% if index % 5 == 0 %>
      </div>
  <% end %>
<% end %>
